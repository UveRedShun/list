<% if @user_or_list == "1"%>
    <h1>User</h1>
    <table>
        <% @users.each do |user| %>
        <thead>
            <tr>
                <th></th>
                <th><%= user.user_name %> </th>
            </tr>
        </thead>
        <% end %>
    </table>

<% elsif @user_or_list == "2" %>
    <h2>List</h2>
    <table>
    <% @lists.each do |list| %>
        <thead>
            <tr>
                <th></th>
                <th><%= list.list_name %></th>
            </tr>
        </thead>
    <% end %>
    </table>
<% else %>
<% end %>