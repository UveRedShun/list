<main class="listnew-container">
  <%= form_with( model: @Favoritelist ,local: true) do |f| %>
    <div class="listnew-info">
      <div class="listnew-info__list-name">
        <%= f.text_field :list_name, autofocus: true, placeholder: "リストネーム", class:"listnew-info__list-name--form" %>
      </div>   
      <div class="listnew-info__list-comment">
        <%= f.text_field :list_comment, placeholder: "リストコメント", class:"listnew-info__list-comment--form" %>
      </div>
      <div class="listnew-info__list-submit">
        <%= f.submit "リストを追加", class:"listnew-info__list-submit--form" %>
      </div>
    </div>
  <% end %>
</main>

